{
  "executable": "BerryAIGen.Electron",
  "splashscreen": {
    "imageFile": ""
  },
  "name": "BerryAIGen.Electron",
  "author": "BerryAIGen Team",
  "singleInstance": true,
  "environment": "Production",
  "build": {
    "appId": "com.BerryAIGen.Electron",
    "productName": "BerryAIGen Toolkit",
    "copyright": "Copyright Â© 2026 BerryAIGen",
    "buildVersion": "1.0.0",
    "compression": "maximum",
    "directories": {
      "output": "../../../bin/Desktop"
    },
    "extraResources": [
      {
        "from": "./bin",
        "to": "bin",
        "filter": [ "**/*" ]
      }
    ],
    "files": [
      {
        "from": "./ElectronHostHook/node_modules",
        "to": "ElectronHostHook/node_modules",
        "filter": [ "**/*" ]
      },
      "**/*"
    ]
  }
}
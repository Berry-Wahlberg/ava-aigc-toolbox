<ResourceDictionary  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                     xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="AppPrimary">#2196F3</Color>
    <Color x:Key="AppPrimaryLight">#64B5F6</Color>
    <Color x:Key="AppPrimaryDark">#1976D2</Color>
    <Color x:Key="AppSecondary">#7C4DFF</Color>
    <Color x:Key="AppAccent">#FF4081</Color>
    <Color x:Key="AppSuccess">#4CAF50</Color>
    <Color x:Key="AppWarning">#FF9800</Color>
    <Color x:Key="AppError">#F44336</Color>
    <Color x:Key="AppInfo">#00BCD4</Color>

    <Color x:Key="AppBackground">#FAFAFA</Color>
    <Color x:Key="AppSurface">#FFFFFF</Color>
    <Color x:Key="AppSurfaceVariant">#F5F5F5</Color>
    <Color x:Key="AppOnBackground">#212121</Color>
    <Color x:Key="AppOnSurface">#212121</Color>
    <Color x:Key="AppOnSurfaceVariant">#757575</Color>

    <Color x:Key="AppOutline">#E0E0E0</Color>
    <Color x:Key="AppOutlineVariant">#BDBDBD</Color>

    <Color x:Key="AppDisabled">#9E9E9E</Color>
    <Color x:Key="AppDisabledBackground">#F5F5F5</Color>

    <Color x:Key="AppHover">#E3F2FD</Color>
    <Color x:Key="AppPressed">#BBDEFB</Color>
    <Color x:Key="AppFocused">#2196F3</Color>

    <Color x:Key="AppOverlay">#80000000</Color>
    <Color x:Key="AppShadow">#000000</Color>

    <Color x:Key="AppControlBackground">#FFFFFF</Color>
    <Color x:Key="AppSplitter">#E0E0E0</Color>
    <Color x:Key="AppHighlight">#2196F3</Color>
    <Color x:Key="AppHighlightFocused">#1976D2</Color>
    <Color x:Key="AppBgHighlight">#7E2196F3</Color>
    <Color x:Key="AppDisabledForeground">#9E9E9E</Color>
    <Color x:Key="AppForeground">#212121</Color>
    <Color x:Key="AppPreviewBackground">#F5F5F5</Color>
    <Color x:Key="AppToastBackground">#2196F3</Color>

    <BitmapImage  x:Key="CopyIcon" CreateOptions="IgnoreImageCache" CacheOption="OnLoad" UriSource="pack://application:,,,/Icons/Light/copy-32.png" />
    <BitmapImage  x:Key="FavoriteIcon" CreateOptions="IgnoreImageCache" CacheOption="OnLoad" UriSource="pack://application:,,,/Icons/Light/favorite-32.png" />
    <BitmapImage  x:Key="FilterIcon" CreateOptions="IgnoreImageCache" CacheOption="OnLoad" UriSource="pack://application:,,,/Icons/Light/filter-32.png" />
    <BitmapImage  x:Key="LinkIcon" CreateOptions="IgnoreImageCache" CacheOption="OnLoad" UriSource="pack://application:,,,/Icons/Light/link-32.png" />
    <BitmapImage  x:Key="ModelsIcon" CreateOptions="IgnoreImageCache" CacheOption="OnLoad" UriSource="pack://application:,,,/Icons/Light/models-32.png" />
    <BitmapImage  x:Key="RebuildIcon" CreateOptions="IgnoreImageCache" CacheOption="OnLoad" UriSource="pack://application:,,,/Icons/Light/rebuild-32.png" />
    <BitmapImage  x:Key="ScanFolderIcon" CreateOptions="IgnoreImageCache" CacheOption="OnLoad" UriSource="pack://application:,,,/Icons/Light/scan-folder-32.png" />
    <BitmapImage  x:Key="SearchIcon" CreateOptions="IgnoreImageCache" CacheOption="OnLoad" UriSource="pack://application:,,,/Icons/Light/search-32.png" />
    <BitmapImage  x:Key="SettingsIcon" CreateOptions="IgnoreImageCache" CacheOption="OnLoad" UriSource="pack://application:,,,/Icons/Light/settings-32.png" />
    <BitmapImage  x:Key="TrashIcon" CreateOptions="IgnoreImageCache" CacheOption="OnLoad" UriSource="pack://application:,,,/Icons/Light/trash-32.png" />

</ResourceDictionary>



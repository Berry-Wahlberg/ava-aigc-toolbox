<ResourceDictionary  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
     xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="AppPrimary">#64B5F6</Color>
    <Color x:Key="AppPrimaryLight">#90CAF9</Color>
    <Color x:Key="AppPrimaryDark">#42A5F5</Color>
    <Color x:Key="AppSecondary">#B388FF</Color>
    <Color x:Key="AppAccent">#FF80AB</Color>
    <Color x:Key="AppSuccess">#81C784</Color>
    <Color x:Key="AppWarning">#FFB74D</Color>
    <Color x:Key="AppError">#E57373</Color>
    <Color x:Key="AppInfo">#4DD0E1</Color>

    <Color x:Key="AppBackground">#121212</Color>
    <Color x:Key="AppSurface">#1E1E1E</Color>
    <Color x:Key="AppSurfaceVariant">#2C2C2C</Color>
    <Color x:Key="AppOnBackground">#E0E0E0</Color>
    <Color x:Key="AppOnSurface">#E0E0E0</Color>
    <Color x:Key="AppOnSurfaceVariant">#B0B0B0</Color>

    <Color x:Key="AppOutline">#404040</Color>
    <Color x:Key="AppOutlineVariant">#606060</Color>

    <Color x:Key="AppDisabled">#616161</Color>
    <Color x:Key="AppDisabledBackground">#2C2C2C</Color>

    <Color x:Key="AppHover">#2C3E50</Color>
    <Color x:Key="AppPressed">#34495E</Color>
    <Color x:Key="AppFocused">#64B5F6</Color>

    <Color x:Key="AppOverlay">#80000000</Color>
    <Color x:Key="AppShadow">#000000</Color>

    <Color x:Key="AppControlBackground">#2D2D30</Color>
    <Color x:Key="AppSplitter">#404040</Color>
    <Color x:Key="AppHighlight">#64B5F6</Color>
    <Color x:Key="AppHighlightFocused">#90CAF9</Color>
    <Color x:Key="AppBgHighlight">#7E2D2D30</Color>
    <Color x:Key="AppDisabledForeground">#616161</Color>
    <Color x:Key="AppForeground">#E0E0E0</Color>
    <Color x:Key="AppPreviewBackground">#252526</Color>
    <Color x:Key="AppToastBackground">#64B5F6</Color>

    <BitmapImage  x:Key="CopyIcon" CreateOptions="IgnoreImageCache" CacheOption="OnLoad" UriSource="pack://application:,,,/Icons/Dark/copy-32.png" />
    <BitmapImage  x:Key="FavoriteIcon" CreateOptions="IgnoreImageCache" CacheOption="OnLoad" UriSource="pack://application:,,,/Icons/Dark/favorite-32.png" />
    <BitmapImage  x:Key="FilterIcon" CreateOptions="IgnoreImageCache" CacheOption="OnLoad" UriSource="pack://application:,,,/Icons/Dark/filter-32.png" />
    <BitmapImage  x:Key="LinkIcon" CreateOptions="IgnoreImageCache" CacheOption="OnLoad" UriSource="pack://application:,,,/Icons/Dark/link-32.png" />
    <BitmapImage  x:Key="ModelsIcon" CreateOptions="IgnoreImageCache" CacheOption="OnLoad" UriSource="pack://application:,,,/Icons/Dark/models-32.png" />
    <BitmapImage  x:Key="RebuildIcon" CreateOptions="IgnoreImageCache" CacheOption="OnLoad" UriSource="pack://application:,,,/Icons/Dark/rebuild-32.png" />
    <BitmapImage  x:Key="ScanFolderIcon" CreateOptions="IgnoreImageCache" CacheOption="OnLoad" UriSource="pack://application:,,,/Icons/Dark/scan-folder-32.png" />
    <BitmapImage  x:Key="SearchIcon" CreateOptions="IgnoreImageCache" CacheOption="OnLoad" UriSource="pack://application:,,,/Icons/Dark/search-32.png" />
    <BitmapImage  x:Key="SettingsIcon" CreateOptions="IgnoreImageCache" CacheOption="OnLoad" UriSource="pack://application:,,,/Icons/Dark/settings-32.png" />
    <BitmapImage  x:Key="TrashIcon" CreateOptions="IgnoreImageCache" CacheOption="OnLoad" UriSource="pack://application:,,,/Icons/Dark/trash-32.png" />

</ResourceDictionary>


